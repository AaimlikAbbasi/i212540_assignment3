<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4"
    android:background="#CEF5FA">




    <RelativeLayout
        android:id="@+id/rectangle9"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        >

        <ImageView
            android:id="@+id/backgroundImageView"

            android:layout_width="100dp"
            android:layout_height="130dp"
            android:src="@drawable/twocircle2"/>

        <ImageView
            android:id="@+id/arrowImageView9"
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_marginStart="3dp"
            android:layout_marginTop="10dp"
            android:rotation="-135"
            android:src="@drawable/arrow_right_top_svgrepo_com"
            android:contentDescription="Arrow Icon"
            app:tint="#0B7C71" />

    </RelativeLayout>


    <TextView
        android:id="@+id/main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:paddingTop="20dp"
        android:text="Add New Mentor"
        android:textColor="#0B7C71"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/phonePrefix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="120dp"
        android:text="Help us expand!"
        android:textAlignment="center"
        android:textColor="#0B7C71"
        android:textSize="20sp" />




    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/phonePrefix"
        android:layout_marginTop="16dp"
        android:background="@drawable/acticity_10_rectangle">

        <RelativeLayout
            android:layout_width="100dp"
            android:layout_height="200dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/activity11layout"
            android:layout_marginBottom="460dp">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="92dp"
                android:layout_height="44dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="472dp"
                app:srcCompat="@android:drawable/presence_video_online"
                app:tint="#0B7C71" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="100dp"
            android:id="@+id/one"
            android:layout_height="200dp"
            android:layout_marginStart="230dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/activity11layout"
            android:layout_marginBottom="460dp">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="18dp"
                android:layout_marginBottom="462dp"
                android:background="@drawable/drawable"
                android:src="@drawable/camera_svgrepo_com" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/first"
            android:layout_marginTop="110dp"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="3dp"
                android:text="Name"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="50dp"
                android:background="@drawable/rounded_edittext_background"
                android:drawablePadding="8dp"
                android:hint="Enter Name"
                android:padding="8dp"
                android:textColor="@color/black" />

        </RelativeLayout>



        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/second"
            android:layout_below="@+id/first"
            android:layout_marginTop="1dp"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="-3dp"
                android:text="Description"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/name1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="50dp"
                android:background="@drawable/rounded_edittext_background"
                android:drawablePadding="8dp"
                android:hint="Enter Description"
                android:padding="8dp"
                android:textColor="@color/black" />

        </RelativeLayout>



        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/third"
            android:layout_below="@+id/second"
            android:layout_marginTop="1dp"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="3dp"
                android:text="Status"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <Spinner
                android:id="@+id/name2"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="50dp"
                android:background="@drawable/rounded_edittext_background"
                android:drawablePadding="8dp"
                android:hint="Available"
                android:padding="8dp"
                android:textColor="@color/black" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/sessioncost"
            android:layout_below="@+id/third"
            android:layout_marginTop="1dp"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="3dp"
                android:text="Session Cost"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/name3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="50dp"
                android:background="@drawable/rounded_edittext_background"
                android:drawablePadding="8dp"
                android:hint="Enter Cost"
                android:padding="8dp"
                android:textColor="@color/black" />

        </RelativeLayout>


        <TextView
            android:id="@+id/loginButton3"
            android:layout_width="250dp"
            android:layout_height="48dp"
            android:layout_below="@+id/sessioncost"
            android:gravity="center"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="4dp"
            android:backgroundTint="#0B7C71"
            android:background="@drawable/activity10_rec"
            android:text="Upload"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            />




        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_below="@id/loginButton3">

            <!-- Grey rectangle background -->
            <RelativeLayout
                android:id="@+id/rectangleBackground"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/sevenrectangle">

                <!-- Icon 1: Home -->
                <ImageView
                    android:id="@+id/icon1"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="20dp"
                    app:srcCompat="@drawable/home_icon_silhouette_svgrepo_com" />

                <!-- Name for Icon 1: Home -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/icon1"
                    android:textColor="@color/white"
                    android:layout_marginStart="30dp"
                    android:layout_centerHorizontal="false"
                    android:text="Home" />

                <!-- Icon 3: Search -->
                <ImageView
                    android:id="@+id/icon3"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="20dp"
                    android:layout_toEndOf="@id/icon1"
                    android:layout_marginStart="50dp"
                    app:tint="#D7D0D8"
                    app:srcCompat="?android:attr/actionModeWebSearchDrawable" />

                <!-- Name for Icon 3: Search -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/icon3"
                    android:textColor="@color/white"
                    android:layout_marginStart="120dp"
                    android:layout_centerHorizontal="false"
                    android:text="Search" />

                <!-- Icon 4: Chat -->
                <ImageView
                    android:id="@+id/icon4"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="20dp"
                    android:layout_toEndOf="@id/icon3"
                    android:layout_marginStart="50dp"
                    app:tint="#D7D0D8"
                    app:srcCompat="@android:drawable/stat_notify_chat" />

                <!-- Name for Icon 4: Chat -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/icon4"
                    android:textColor="@color/white"
                    android:layout_marginStart="225dp"
                    android:layout_centerHorizontal="false"
                    android:text="Chat" />

                <!-- Icon 2: Profile -->
                <ImageView
                    android:id="@+id/icon2"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="20dp"
                    android:layout_toEndOf="@id/icon4"
                    android:layout_marginStart="50dp"
                    app:srcCompat="@drawable/user_profile_svgrepo_com" />

                <!-- Name for Icon 2: Profile -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/icon2"
                    android:textColor="@color/white"
                    android:layout_marginStart="320dp"
                    android:layout_centerHorizontal="false"
                    android:text="Profile" />


            </RelativeLayout>
        </RelativeLayout>
        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:textColor="@color/white"
            android:layout_marginTop="475dp"
            android:layout_marginStart="170dp"
            android:background="@drawable/small_circle"
            android:src="@drawable/plus"
            app:tint="@android:color/white" />





    </RelativeLayout>



</RelativeLayout>



